# Partial Exam 2 - Superpower

def superpower(a,b):
        x = 1
        y = a
        while (x<b):
                y = y * a
                x = x + 1
        return y

print()
x = int(input("Give me the first number: "))
y = int(input("Give me the second number: "))

do_sup = superpower(x,y)
print() 
print ("RESULT:", do_sup)
