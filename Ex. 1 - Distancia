# Aram Rodrigo Curiel Graxiola
# A01229982
# Partial Exam 2 - Distancia

import math

def distancia(a,b,c,d):
	x = c - a
	y = d - b
	z = (x**2) + (y**2)
	sq = math.sqrt(z)
	return sq

print()
num1 = int(input("Give me the X1 value: "))
num2 = int(input("Give me the Y1 value: "))
num3 = int(input("Give me the X2 value: "))
num4 = int(input("Give me the Y2 value: "))

dist = distancia (num1,num2,num3,num4)

print()
print("The result is:", dist)
